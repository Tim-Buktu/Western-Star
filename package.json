{
  "name": "western-star-landing",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend:install": "cd backend && npm install",
    "backend:setup": "cd backend && chmod +x setup.sh && ./setup.sh",
    "backend:dev": "cd backend && npm run dev",
    "backend:migrate": "cd backend && npm run migrate",
    "backend:seed": "cd backend && npm run seed",
    "backend:studio": "cd backend && npm run studio",
    "full-setup": "npm install && npm run backend:setup",
    "dev:all": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.441.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1"
  }
}
